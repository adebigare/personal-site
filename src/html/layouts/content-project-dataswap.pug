- var headlineClasses = ['project-hero__headline', 'project-hero_headline_accent']
//include partials/hero
// Later when bug fixed, add hero as block/extend
include ./../partials/header
include ./../partials/primary-nav
body
	//- include partials/primary-nav
	main

		article.project-hero.project-hero-background_exp
			img(class='full-width project-hero__img' src=require("./../../assets/images/dataswap-header.png"))
			h2(class=headlineClasses) The Data Swap
			div.dots

		article.content
			aside.highlights
				.highlights__grid
					.titleA
						h3.highlights__title Contributions
					.highlights__list.dataA
						ul
							each item in ["Event Management", "Design", "Development", "Communications", "Leadership", "Strategy"]
								li.highlights__skill.p=item
					.titleB
						h3.highlights__title Main Skills
					.highlights__list.dataB
						// TO DO: Data integration. Why is the "each" printing each letter?
						//ul
						// 	each item in `${data.projectSkills}`
						// 		li=item
						ul
							each item in ["HTML/CSS", "Identity Design", "Event Design", "Event/Project Planning", "Copy Writing", "Community Organizing"]
								li.highlights__skill.p=item
			section.project-writeup
				.project-md
					include:markdown-it(linkify html typographer) ./../../assets/data/data-swap.md
	include ./../partials/footer


